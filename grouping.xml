  	<parameter name="browser"  value="chrome"/> 
  	<groups>
          <run>
              <include name="Master"/>
               <!-- <include name="Sanity" />-->
               <!-- below line regression executes AccountRegistrationTestGroup-->
			  <!--  <include name="Regression" /> --> 
