          </run>
       </groups>
    <classes>
     <class name="testCases.TC_001_AccountRegistrationTestGroup"/>
     <class name="testCases.TC_002_LoginTestGroup"/>
         <!--  <class name="testCases.TC_003_LoginDDTGroup"/>-->
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
